  <ItemGroup>
    <None Include="wwwroot\scss\custom.scss" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AspNetCore.SassCompiler" Version="1.75.0" />
  </ItemGroup>

</Project>
